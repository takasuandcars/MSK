      <%= form_with(model: @pickup_request, remote: true, class: "form_with_css") do |f| %>
                <div class="tr td pickup_date_row"><%= f.date_field :pickup_date %></div>
                <div class="tr td car_type_row"><%= f.text_field :car_type %></div>
                <div class="tr td number_of_car_row"><%= f.text_field :number_of_car %></div>
                <div class="tr td number_of_total_order_row"><%= @pickup_request.total_order %></div>
                <div class="tr td weight_row"><%= f.text_field :weight %></div>
                <div class="tr td cbm_row"><%= f.text_field :cbm %></div>
                <div class="tr td emark_column"><%= f.text_field :note%></div>
                <div class="tr td function_row">
                    <span><%= f.submit "保存", class: "btn"%></span>
                   
                    <span><%= link_to "削除", pickup_request_path(id: @pickup_request.id), method: :delete, class: "btn", remote: true%></span>
                </div>
        <%end%>  
        